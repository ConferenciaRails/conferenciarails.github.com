---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Conferencia Rails 2010</title>
 <link href="http://www.conferenciarails.com/atom.xml" rel="self"/>
 <link href="http://www.conferenciarails.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.conferenciarails.com</id>
 <author>
   <name>Asociación para la promoción del desarrollo web (APRODEWEB)</name>
   <email>info@aprodeweb.es</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://conferenciarails.com/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>

